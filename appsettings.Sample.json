{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=dbname;Username=dbuser;Password=dbpass;TrustServerCertificate=True;"
  },
  "App": {
    "TimeZone": "Asia/Bishkek"
  },
  "Api": {
    "RateLimit": 100,
    "AuthRateLimit": 10
  },
  "Jwt": {
    "Key": "secret-key-123456-123456-123456-123456",
    "RefreshTokenKey": "refresh-token-secret-key-123456-123456-123456-123456",
    "Issuer": "api.domain.com",
    "Audience": "domain.com",
    "AccessTokenExpMins": 1,
    "RefreshTokenExpDays": 180
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000"
    ]
  }
}
